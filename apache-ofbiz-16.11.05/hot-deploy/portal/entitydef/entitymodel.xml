    <!--    Enriry to manage user login access token-->
    <entity entity-name="AutopattUserLoginSessionData" package-name="com.autopatt.portal" title="User Login Session Data">
        <field name="userLoginId" type="very-long"/>
        <field name="sessionData" type="very-long"/>
        <prim-key field="sessionData"/>
